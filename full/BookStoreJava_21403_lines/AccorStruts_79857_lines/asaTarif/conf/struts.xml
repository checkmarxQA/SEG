<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC   
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"   
    "http://struts.apache.org/dtds/struts-2.0.dtd"> 

<struts>
    <package name="rateDefaultPackage" extends="struts-default" >
        <interceptors>
            <interceptor name="checkSessionInterceptor" class="com.accor.asa.rate.interceptor.CheckSessionInterceptor"/>
            <interceptor-stack name="rateDefaultPackageStack">
                <interceptor-ref name="checkSessionInterceptor"/>
                <interceptor-ref name="paramsPrepareParamsStack"/>
            </interceptor-stack>
        </interceptors>
        <default-interceptor-ref name="rateDefaultPackageStack"/>
        <global-results>
            <result name="welcome">/index.html</result>
            <result name="error">/error.jsp</result>
        </global-results>
    </package>
</struts>
